{
  "name": "opentrack-website-2018",
  "version": "1.0.0",
  "description": "Jekyll, Bootstrap4, Webpack4 website",
  "main": "assets/entry.js",
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,scss,html}": [
      "editorconfig-tools fix",
      "git add"
    ]
  },
  "scripts": {
    "develop": "webpack-dev-server --config webpack.dev.js",
    "buildold": "webpack --config webpack.prod.js",
    "lint": "eslint",
    "lint:staged": "lint-staged",
    "clean": "shx rm -rf _site/",
    "scaffold": "npm run clean && shx mkdir -p assets && shx cp -r _assets/img/. assets/img",
    "build": "npm run scaffold && cross-env NODE_ENV=production webpack -p --progress && bundle exec jekyll build",
    "build:debug": "npm run scaffold && webpack --progress -d --verbose && bundle exec jekyll build -t",
    "start": "npm run scaffold && concurrently -k -n \"WEBPACK,JEKYLL,BROWSER-SYNC\" -c \"bgBlue.bold,bgRed.bold,bgCyan.bold\" \"webpack -w --mode=development\" \"bundle exec jekyll build --watch\" \"browser-sync start -s _site -f _site --extensions html\""
  },
  "repository": "https://github.com/twobyte/openath.github.io",
  "author": "Toby Trembath",
  "license": "MIT",
  "dependencies": {
    "aos": "^2.3.2",
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "octicons": "^7.3.0",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "autoprefixer": "^8.5.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "browser-sync": "^2.24.4",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "lint-staged": "^7.1.0",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "sass-loader": "^7.0.1",
    "serviceworker-webpack-plugin": "^1.0.0-alpha02",
    "shx": "^0.2.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
